     SuperscriptBox["\[Epsilon]", "\[Mu]\[Nu]\[Rho]"], "(", 
     RowBox[{
      SubscriptBox["p", 
       RowBox[{"1", "\[Rho]"}]], 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", 
         SubsuperscriptBox["p", "1", "\[Nu]"]}]], 
       RowBox[{"+", 
        SubscriptBox["p", 
         RowBox[{"2", "\[Rho]"}]]}]}], 
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", 
        SubsuperscriptBox["p", "2", "\[Nu]"]}]]}], ")"}]}], TraditionalForm]],
  
  FormatType->TraditionalForm,ExpressionUUID->
