   RowBox[{"F4series", "[", 
    RowBox[{"n1_", ",", "n2_"}], "]"}], "=", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"F4", "[", 
      RowBox[{
      "\[Alpha]_", ",", "\[Beta]_", ",", "\[Gamma]1_", ",", "\[Gamma]2_"}], 
