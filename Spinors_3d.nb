Cell[TextData[{
 "After contracting with polarization spinors, the 3-point function can be \
written\nin terms of the polarization tensors ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["Z", 
    RowBox[{"j", ",", "m"}]], TraditionalForm]],ExpressionUUID->
  "0e5361d4-c3a3-4d06-abb7-ec781a6bc414"],
 ":\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["\[Zeta]", 
       SubscriptBox["a", "1"]], "\[CenterDot]", "\[CenterDot]", 
      "\[CenterDot]", 
      SubscriptBox["\[Zeta]", 
       SubscriptBox["a", 
        RowBox[{"2", "j"}]]]}], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"\[LeftAngleBracket]", " ", 
       RowBox[{
        RowBox[{
         SuperscriptBox["O", 
          RowBox[{
           SubscriptBox["a", "1"], "\[CenterDot]", "\[CenterDot]", 
           "\[CenterDot]", 
           SubscriptBox["a", 
            RowBox[{"2", "j"}]]}]], "(", 
         RowBox[{"-", "p"}], ")"}], 
        RowBox[{
         SubscriptBox["\[Phi]", "1"], "(", 
         SubscriptBox["p", "1"], ")"}], " ", 
        RowBox[{
         SubscriptBox["\[Phi]", "2"], "(", 
         SubscriptBox["p", "2"], ")"}]}], " ", "\[RightAngleBracket]"}], 
      "\[RightAngleBracket]"}]}], "=", 
    RowBox[{
     SuperscriptBox["s", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[CapitalDelta]", "1"], "+", 
         SubscriptBox["\[CapitalDelta]", "2"], "+", "\[CapitalDelta]", "-", 
         "6", "-", "j"}], ")"}], "/", "2"}]], 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"m", "=", 
        RowBox[{"-", "j"}]}], "j"], 
      RowBox[{
       RowBox[{
        SubscriptBox["F", 
         RowBox[{"j", ",", "m"}]], "(", 
        RowBox[{
         SubscriptBox["w", "1"], ",", 
         SubscriptBox["w", "2"]}], ")"}], 
       SubscriptBox["Z", 
        RowBox[{"j", ",", "m"}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "a6c092b4-9466-4954-980f-8c9fc039238d"],
 "\n\nThe functions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     RowBox[{"j", ",", "m"}]], "(", 
    RowBox[{
     SubscriptBox["w", "1"], ",", 
     SubscriptBox["w", "2"]}], ")"}], TraditionalForm]],ExpressionUUID->
  "047be6c4-9020-462c-a16a-e39049c3353a"],
 " will be fixed by conformal symmetry\n(up to an overall normalization \
factor)"
}], "Text",
 CellChangeTimes->{{3.846214190653*^9, 3.8462144952165823`*^9}, {
  3.8462181651057377`*^9, 3.846218179477355*^9}, {3.846218243020316*^9, 
