         SubsuperscriptBox["\[Sum]", 
          RowBox[{"i2", "=", "0"}], "n2"], 
         RowBox[{
          FractionBox[
           RowBox[{
            RowBox[{"Pochhammer", "[", 
             RowBox[{"\[Alpha]", ",", 
              RowBox[{"i1", "+", "i2"}]}], "]"}], 
            RowBox[{"Pochhammer", "[", 
             RowBox[{"\[Beta]", ",", 
              RowBox[{"i1", "+", "i2"}]}], "]"}]}], 
           RowBox[{
            RowBox[{"i1", "!"}], 
            RowBox[{"i2", "!"}], 
            RowBox[{"Pochhammer", "[", 
             RowBox[{"\[Gamma]1", ",", "i1"}], "]"}], 
            RowBox[{"Pochhammer", "[", 
             RowBox[{"\[Gamma]2", ",", "i2"}], "]"}]}]], 
          SuperscriptBox["w1", "i1"], 
          SuperscriptBox["w2", "i2"]}]}]}]}], "]"}]}], "\[IndentingNewLine]", 
    "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
