    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox["\[Xi]\[Lambda]bar", "\[Lambda]\[Lambda]bar"]}], " ", 
      RowBox[{"\[Lambda]bar", "[", "a", "]"}], 
      RowBox[{"d\[Xi]", "[", 
