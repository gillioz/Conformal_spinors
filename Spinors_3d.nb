Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"k3", "[", 
    RowBox[{"a_", ",", "b_"}], "]"}], "[", "w_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"d\[Lambda]", "[", 
          RowBox[{
           RowBox[{"d\[Lambda]bar", "[", 
            RowBox[{"w", ",", "a"}], "]"}], ",", "b"}], "]"}], "+", 
         RowBox[{"d\[Lambda]bar", "[", 
