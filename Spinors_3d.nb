Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"k3", "[", 
    RowBox[{"a_", ",", "b_"}], "]"}], "[", "w_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"d\[Lambda]", "[", 
          RowBox[{
           RowBox[{"d\[Lambda]bar", "[", 
            RowBox[{"w", ",", "a"}], "]"}], ",", "b"}], "]"}], "+", 
         RowBox[{"d\[Lambda]bar", "[", 
          RowBox[{
           RowBox[{"d\[Lambda]", "[", 
            RowBox[{"w", ",", "a"}], "]"}], ",", "b"}], "]"}]}], ")"}]}]}], 
     "\[IndentingNewLine]", "-", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"2", "\[Lambda]\[Lambda]bar"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"\[Lambda]", "[", "a", "]"}], 
         RowBox[{"d\[Lambda]", "[", 
          RowBox[{"w", ",", "b"}], "]"}]}], "+", 
        RowBox[{
         RowBox[{"\[Lambda]", "[", "b", "]"}], 
         RowBox[{"d\[Lambda]", "[", 
          RowBox[{"w", ",", "a"}], "]"}]}], "-", 
        RowBox[{
         RowBox[{"\[Lambda]bar", "[", "a", "]"}], 
         RowBox[{"d\[Lambda]bar", "[", 
          RowBox[{"w", ",", "b"}], "]"}]}], "-", 
        RowBox[{
         RowBox[{"\[Lambda]bar", "[", "b", "]"}], 
         RowBox[{"d\[Lambda]bar", "[", 
          RowBox[{"w", ",", "a"}], "]"}]}]}], ")"}]}], "\[IndentingNewLine]", 
     "+", 
     RowBox[{"2", 
      FractionBox[
       RowBox[{"p1", "[", 
        RowBox[{"a", ",", "b"}], "]"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "x", "-", "y"}], ")"}], 
        SuperscriptBox["\[Lambda]\[Lambda]bar", "2"]}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"x", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", "-", "x"}], ")"}], "2"], " ", 
         RowBox[{"D", "[", 
          RowBox[{"w", ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "2"}], "}"}]}], "]"}]}], "-", 
        RowBox[{
         SuperscriptBox["y", "2"], 
         RowBox[{"(", 
          RowBox[{"1", "-", "y"}], ")"}], " ", 
         RowBox[{"D", "[", 
          RowBox[{"w", ",", 
           RowBox[{"{", 
            RowBox[{"y", ",", "2"}], "}"}]}], "]"}]}], "\[IndentingNewLine]", 
        "-", 
        RowBox[{
         FractionBox["1", "2"], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", "-", "x"}], ")"}], "2"], 
         RowBox[{"D", "[", 
          RowBox[{"w", ",", "x"}], "]"}]}], "-", 
        RowBox[{
         FractionBox["1", "2"], 
         SuperscriptBox["y", "2"], " ", 
         RowBox[{"D", "[", 
          RowBox[{"w", ",", "y"}], "]"}]}]}], ")"}]}], "\[IndentingNewLine]", 
     "+", 
     RowBox[{"2", 
      FractionBox[
       RowBox[{"p2", "[", 
        RowBox[{"a", ",", "b"}], "]"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "x", "-", "y"}], ")"}], 
        SuperscriptBox["\[Lambda]\[Lambda]bar", "2"]}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox["x", "2"]}], 
         RowBox[{"(", 
          RowBox[{"1", "-", "x"}], ")"}], " ", 
         RowBox[{"D", "[", 
          RowBox[{"w", ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "2"}], "}"}]}], "]"}]}], "+", 
        RowBox[{"y", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", "-", "y"}], ")"}], "2"], " ", 
         RowBox[{"D", "[", 
          RowBox[{"w", ",", 
           RowBox[{"{", 
            RowBox[{"y", ",", "2"}], "}"}]}], "]"}]}], "\[IndentingNewLine]", 
        "-", 
        RowBox[{
         FractionBox["1", "2"], 
         SuperscriptBox["x", "2"], 
         RowBox[{"D", "[", 
          RowBox[{"w", ",", "x"}], "]"}]}], "-", 
