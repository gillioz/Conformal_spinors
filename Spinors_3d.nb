Cell[TextData[{
 "The antisymmetric scalar product for spinors is defined by:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Zeta]", "1"], "\[CenterDot]", 
     SubscriptBox["\[Zeta]", "2"]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Zeta]", "1", "a"], 
      SubscriptBox["\[Zeta]", 
       RowBox[{"2", "a"}]]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        SubscriptBox["\[Zeta]", 
         RowBox[{"1", "a"}]]}], 
       SubsuperscriptBox["\[Zeta]", "2", "a"]}], "=", 
      RowBox[{
       RowBox[{"-", 
        SubscriptBox["\[Zeta]", "2"]}], "\[CenterDot]", 
       SubscriptBox["\[Zeta]", "1"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "921125fe-d3f9-4d6f-9c01-9888b9c1a3c5"]
}], "Text",
