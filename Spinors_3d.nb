          RowBox[{"\[Xi]\[Lambda]bar", " ", 
           RowBox[{"\[Lambda]", "[", "a", "]"}]}], "+", 
          RowBox[{"\[Xi]\[Lambda]", " ", 
           RowBox[{"\[Lambda]bar", "[", "a", "]"}]}]}], ")"}], 
        RowBox[{"2", "\[Lambda]\[Lambda]bar"}]]}], 
      RowBox[{"d\[Xi]", "[", 
