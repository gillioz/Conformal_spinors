Cell[TextData[{
 "The generator of Lorentz transformations can be expressed in terms of p as\n\
\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["M", "\[Mu]"], "=", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "\[Mu]\[Nu]\[Rho]"], 
     SubscriptBox["p", "\[Rho]"], 
     FractionBox["\[PartialD]", 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["p", "\[Nu]"]}]]}]}], TraditionalForm]],ExpressionUUID->
  "34a89cc0-753b-4abf-9e36-d81f75e2d432"],
 "\n\t\nThis is verified by the following identities :\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["M", "\[Mu]"], 
     SuperscriptBox["p", "\[Alpha]"]}], "=", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "\[Mu]\[Alpha]\[Rho]"], 
     SubscriptBox["p", "\[Rho]"]}]}], TraditionalForm]],ExpressionUUID->
  "d8364679-d6a6-4c82-b9be-26d1d30b2032"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["M", "\[Mu]"], 
     SuperscriptBox["p", "2"]}], "=", "0"}], TraditionalForm]],ExpressionUUID->
  "05e96a24-05b0-4a80-aa1d-2e6d907860fa"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["M", "\[Mu]"], "(", 
     RowBox[{"z", "\[CenterDot]", "p"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[Epsilon]", "\[Mu]\[Nu]\[Rho]"], 
      SubscriptBox["p", "\[Rho]"], 
      SuperscriptBox["z", "\[Nu]"]}], "=", 
     RowBox[{
      SuperscriptBox["\[CapitalSigma]", "\[Mu]"], "(", 
      RowBox[{"z", "\[CenterDot]", "p"}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"d257590a-8d1f-40dc-a1e2-b9b02e926bc3"]
}], "Text",
