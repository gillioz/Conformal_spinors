Cell[TextData[{
 "This system is solved by the generalized hypergeometric function ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", "4"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "21546d2b-47ca-45dd-8954-978adc533af1"],
 ",\nwhich can be expanded into an infinite series of factorized functions \n\
\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "4"], "(", 
     RowBox[{"\[Alpha]", ",", "\[Beta]", ",", 
      SubscriptBox["\[Gamma]", "1"], ",", 
      RowBox[{
       SubscriptBox["\[Gamma]", "2"], ";", "x"}], ",", "y"}], ")"}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"n", "=", "0"}], "\[Infinity]"], 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox[
         RowBox[{"(", "\[Alpha]", ")"}], "n"], 
        SubscriptBox[
         RowBox[{"(", "\[Beta]", ")"}], "n"], 
        SubscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Alpha]", "+", "\[Beta]", "-", 
           SubscriptBox["\[Gamma]", "1"], "-", 
           SubscriptBox["\[Gamma]", "2"], "+", "1"}], ")"}], "n"]}], 
       RowBox[{
        RowBox[{"n", "!"}], 
        SubscriptBox[
         RowBox[{"(", 
          SubscriptBox["\[Gamma]", "1"], ")"}], "n"], 
        SubscriptBox[
         RowBox[{"(", 
          SubscriptBox["\[Gamma]", "2"], ")"}], "n"]}]], 
      SuperscriptBox["x", "n"], 
      SubscriptBox[
       RowBox[{
        SuperscriptBox["y", "n"], " "}], "2"], 
      SubscriptBox[
       RowBox[{
        RowBox[{
         SubscriptBox["F", "1"], "(", 
         RowBox[{
          RowBox[{"\[Alpha]", "+", "n"}], ",", 
          RowBox[{"\[Beta]", "+", "n"}], ",", 
          RowBox[{
           RowBox[{
            SubscriptBox["\[Gamma]", "1"], "+", "n"}], ";", "x"}]}], ")"}], 
        " "}], "2"], 
      RowBox[{
       SubscriptBox["F", "1"], "(", 
       RowBox[{
        RowBox[{"\[Alpha]", "+", "n"}], ",", 
        RowBox[{"\[Beta]", "+", "n"}], ",", 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Gamma]", "2"], "+", "n"}], ";", "y"}]}], 
       ")"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3707f894-ff53-4c47-a6d3-d03d5f31461f"],
