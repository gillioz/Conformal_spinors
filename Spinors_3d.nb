Cell[TextData[{
 "The generator of Lorentz transformations is defined as\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["M", "\[Mu]"], "=", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "\[Mu]\[Nu]\[Rho]"], 
     SubscriptBox["p", "\[Rho]"], 
     FractionBox["\[PartialD]", 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["p", "\[Nu]"]}]]}]}], TraditionalForm]],ExpressionUUID->
  "1f781f9f-fdaa-4f20-9747-a424bb50e708"],
 "\nwhich in terms of polarization tensors is\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["M", "ab"], "=", 
    RowBox[{
     SuperscriptBox["\[Lambda]", 
      RowBox[{"(", "a"}]], 
     RowBox[{
      SuperscriptBox["\[PartialD]", 
       RowBox[{"b", ")"}]], 
      RowBox[{"+", 
       SuperscriptBox[
        OverscriptBox["\[Lambda]", "_"], 
        RowBox[{"(", "a"}]]}]}], 
     SuperscriptBox[
      OverscriptBox["\[PartialD]", "_"], 
      RowBox[{"b", ")"}]]}]}], TraditionalForm]],ExpressionUUID->
  "50d2ce41-6909-4c67-9602-a08bd188523f"]
}], "Text",
 CellChangeTimes->{{3.834647112448162*^9, 3.834647121670833*^9}, {
