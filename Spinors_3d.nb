Cell[TextData[{
 "The generator of Lorentz transformations is given by\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["M", "\[Mu]"], "=", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "\[Mu]\[Nu]\[Rho]"], "(", 
     RowBox[{
      SubscriptBox["p", 
       RowBox[{"1", "\[Rho]"}]], 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", 
         SubsuperscriptBox["p", "1", "\[Nu]"]}]], 
       RowBox[{"+", 
        SubscriptBox["p", 
         RowBox[{"2", "\[Rho]"}]]}]}], 
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", 
        SubsuperscriptBox["p", "2", "\[Nu]"]}]]}], ")"}]}], TraditionalForm]],
  
  FormatType->TraditionalForm,ExpressionUUID->
  "655ded77-00b5-4f0e-9206-6a54634fe140"],
 "\nIt obeys:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["M", "\[Mu]"], 
     SubsuperscriptBox["p", "1", "\[Alpha]"]}], "=", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "\[Mu]\[Alpha]\[Rho]"], 
     SubscriptBox["p", 
      RowBox[{"1", "\[Rho]"}]]}]}], TraditionalForm]],ExpressionUUID->
  "e7d7a8be-7a36-4509-8aff-e5738e6c8d02"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["M", "\[Mu]"], 
     SubsuperscriptBox["p", "2", "\[Alpha]"]}], "=", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "\[Mu]\[Alpha]\[Rho]"], 
     SubscriptBox["p", 
      RowBox[{"2", "\[Rho]"}]]}]}], TraditionalForm]],ExpressionUUID->
  "c3c15a69-1b12-4c7a-b489-3800a2f9378a"]
}], "Text",
