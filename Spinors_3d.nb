Cell[TextData[{
 "The following second-order differential operator takes a particularly \
simple form in terms of spinors:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["k", "0", "\[Mu]"], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox["p", "\[Mu]"], 
      RowBox[{
       FractionBox[
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{
         RowBox[{"\[PartialD]", 
          SuperscriptBox["p", "\[Nu]"]}], 
         RowBox[{"\[PartialD]", 
          SubscriptBox["p", "\[Nu]"]}]}]], 
       RowBox[{"-", 
        SuperscriptBox["p", "\[Nu]"]}]}], 
      RowBox[{
       FractionBox[
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{
         RowBox[{"\[PartialD]", 
          SuperscriptBox["p", "\[Nu]"]}], 
         RowBox[{"\[PartialD]", 
          SubscriptBox["p", "\[Mu]"]}]}]], 
       RowBox[{"-", 
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", 
          SubscriptBox["p", "\[Mu]"]}]]}]}]}], "=", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "2"]}], 
      SubsuperscriptBox["\[Sigma]", "ab", "\[Mu]"], 
      FractionBox[
       SuperscriptBox["\[PartialD]", "2"], 
       RowBox[{
        RowBox[{"\[PartialD]", 
         SubscriptBox["\[Lambda]", "a"]}], 
        RowBox[{"\[PartialD]", 
         SubscriptBox[
          OverscriptBox["\[Lambda]", "_"], "b"]}]}]]}]}]}], TraditionalForm]],
  ExpressionUUID->"ff6137e3-b08b-4f2c-88b4-798a2b0d6ea2"],
 "\t"
}], "Text",
