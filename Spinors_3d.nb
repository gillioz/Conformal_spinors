Cell[TextData[{
 "After contracting with polarization spinors, the 3-point function can be \
written\nin terms of the polarization tensors ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["Z", 
    RowBox[{"j", ",", "m"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0e5361d4-c3a3-4d06-abb7-ec781a6bc414"],
 ":\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["\[Zeta]", 
       SubscriptBox["a", "1"]], "\[CenterDot]", "\[CenterDot]", 
      "\[CenterDot]", 
      SubscriptBox["\[Zeta]", 
       SubscriptBox["a", 
        RowBox[{"2", "j"}]]]}], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"\[LeftAngleBracket]", " ", 
       RowBox[{
        RowBox[{
         SuperscriptBox["O", 
          RowBox[{
           SubscriptBox["a", "1"], "\[CenterDot]", "\[CenterDot]", 
           "\[CenterDot]", 
           SubscriptBox["a", 
            RowBox[{"2", "j"}]]}]], "(", 
         RowBox[{"-", "p"}], ")"}], 
        RowBox[{
         SubscriptBox["\[Phi]", "1"], "(", 
         SubscriptBox["p", "1"], ")"}], " ", 
        RowBox[{
         SubscriptBox["\[Phi]", "2"], "(", 
         SubscriptBox["p", "2"], ")"}]}], " ", "\[RightAngleBracket]"}], 
      "\[RightAngleBracket]"}]}], "=", 
    RowBox[{
     SuperscriptBox["s", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[CapitalDelta]", "1"], "+", 
         SubscriptBox["\[CapitalDelta]", "2"], "+", 
         SubscriptBox["\[CapitalDelta]", "3"], "-", "6", "-", "j"}], ")"}], 
       "/", "2"}]], 
     SubsuperscriptBox["w", "1", 
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "1"], "-", 
       RowBox[{"d", "/", "2"}]}]], 
     SubsuperscriptBox["w", "2", 
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "2"], "-", 
       RowBox[{"d", "/", "2"}]}]], 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"m", "=", 
        RowBox[{"-", "j"}]}], "j"], 
      RowBox[{
       RowBox[{
        SubscriptBox["F", 
         RowBox[{"j", ",", "m"}]], "(", 
        RowBox[{
         SubscriptBox["w", "1"], ",", 
         SubscriptBox["w", "2"]}], ")"}], 
       SubscriptBox["Z", 
        RowBox[{"j", ",", "m"}]]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a6c092b4-9466-4954-980f-8c9fc039238d"],
