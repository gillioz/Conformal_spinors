Cell[TextData[{
 "There is again a second-order differential operators that is related to \
special conformal transformations:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["K", "\[Mu]"], "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "2"], 
     RowBox[{"(", 
      RowBox[{
       FractionBox["1", "2"], 
       SubsuperscriptBox["p", "i", "\[Mu]"], 
       RowBox[{
        FractionBox[
         SuperscriptBox["\[PartialD]", "2"], 
         RowBox[{
          RowBox[{"\[PartialD]", 
           SubscriptBox["p", "i\[Nu]"]}], 
          RowBox[{"\[PartialD]", 
           SubsuperscriptBox["p", "i", "\[Nu]"]}]}]], 
        RowBox[{"-", 
         SubsuperscriptBox["p", "i", "\[Nu]"]}]}], 
       FractionBox[
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{
         RowBox[{"\[PartialD]", 
          SubscriptBox["p", "i\[Mu]"]}], 
         RowBox[{"\[PartialD]", 
          SubsuperscriptBox["p", "i", "\[Nu]"]}]}]]}], ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"3fa4e531-f67a-408a-9e7a-7b5fc3f0984b"]
