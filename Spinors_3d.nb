Cell[BoxData[{
 RowBox[{
  RowBox[{"H\[Xi]p", "[", "x_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "a", "}"}], ",", 
    RowBox[{
     RowBox[{
      FractionBox["\[Xi]\[Lambda]", "\[Lambda]\[Lambda]bar"], " ", 
      RowBox[{"\[Lambda]", "[", "a", "]"}], 
